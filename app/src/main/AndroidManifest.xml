<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.segundofinal">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SegundoFinal">

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ABMProductosActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".NuevoProductoActivity"
            android:parentActivityName=".ABMProductosActivity" />
        <activity
            android:name=".EditarProductoActivity"
            android:parentActivityName=".ABMProductosActivity" />
        <activity
            android:name=".ABMClientesActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".NuevoClienteActivity"
            android:parentActivityName=".ABMClientesActivity" />
        <activity
            android:name=".EditarClienteActivity"
            android:parentActivityName=".ABMClientesActivity" />
        <activity
            android:name=".NuevaVentaActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".VerReporteResumidoActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".VerReporteDetalladoActivity"
            android:parentActivityName=".MainActivity" />

    </application>

</manifest>